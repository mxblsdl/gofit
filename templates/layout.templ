package templates

templ Layout(title string) {
    <!DOCTYPE html>
    <html>
        <head>
            <title>{ title } | GoFit</title>
            <link rel="stylesheet" href="/static/css/styles.css"/>
            <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        </head>
        <body>
            @Nav()
            <div class="container">
                { children... }
            </div>
        </body>
    </html>
}